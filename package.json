{
	"name": "node-package-template",
	"version": "0.0.0",
	"private": true,
	"description": "Just my personal package template",
	"license": "MIT",
	"author": "Jan T. Sott",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/idleberg/node-package-template.git"
	},
	"keywords": [],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=20"
	},
	"files": [
		"dist/",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "concurrently --prefix-colors blue,green,magenta npm:lint:*",
		"lint:biome": "biome check",
		"lint:knip": "knip --cache --production",
		"lint:publint": "publint",
		"prepack": "npm run build",
		"prepare": "lefthook install",
		"publish:jsr": "deno publish",
		"publish:npm": "np --any-branch",
		"test": "vitest run"
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@idleberg/configs": "^0.3.1",
		"@types/node": "^24.9.0",
		"@vitest/coverage-v8": "^3.2.4",
		"concurrently": "^9.2.1",
		"esbuild": "^0.25.11",
		"knip": "^5.66.2",
		"np": "^10.2.0",
		"publint": "^0.3.15",
		"tsdown": "^0.15.9",
		"vitest": "^3.2.4"
	},
	"packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
